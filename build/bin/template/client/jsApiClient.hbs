const TIMEOUT = 15000;

const defaultOptions = {
  method: 'POST',
  timeout: TIMEOUT,
  headers: { 'Content-Type': 'application/json' },
}

{{#each @root.selfServiceInfos}}
{{#each this.methods}}

export async function {{lowercase this.methodName}}(body, request, options) {
  const path = '{{this.googleHttpOption.router}}';
  const requestOptions = Object.assign({}, defaultOptions, options, { body: JSON.stringify(body) });
  return await request(path, requestOptions);
}
{{/each}}
{{/each}}

export default {
  {{#each @root.selfServiceInfos}}
  {{#each this.methods}}
  {{lowercase this.methodName}},
  {{/each}}
  {{/each}}
}
