{
  "name": "demo",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "apitest": "jest --collectCoverage false --testRegex /src/router/RouterAPITest.ts",
    "deploy": "better-npm-run deploy",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "deploy": {
      "command": "gulp typescript && gulp protoc-copy",
      "env": {
        "DEBUG": "SASDN:*"
      }
    },
    "dev": {
      "command": "npm run deploy && node index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "SASDN:*"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "better-npm-run": "0.1.0",
    "sasdn": "^1.3.4",
    "dotenv": "4.0.0",
    "mockjs": "1.0.1-beta3",
    "sasdn-zipkin": "^1.0.6",
    "sasdn-log": "^1.0.1",
    "sasdn-database": "^1.0.5",
    "protobufjs": "5.0.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "3.2.5",
    "@types/jest": "20.0.8",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "3.0.23",
    "@types/koa-router": "7.0.22",
    "@types/mz": "0.0.31",
    "@types/node": "8.0.31",
    "@types/supertest": "2.0.3",
    "@types/dotenv": "^4.0.2",
    "grpc":"1.8.4",
    "grpc_tools_node_protoc_ts": "2.1.2",
    "gulp": "3.9.1",
    "gulp-copy": "1.0.0",
    "gulp-shell": "0.6.3",
    "gulp-typescript": "3.1.7",
    "jest": "21.0.1",
    "koa": "2.3.0",
    "koa-bodyparser": "4.2.0",
    "koa-router": "7.2.1",
    "merge2": "1.0.3",
    "mz": "2.6.0",
    "supertest": "3.0.0",
    "ts-jest": "21.0.0",
    "typescript": "2.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts|tsx)?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
